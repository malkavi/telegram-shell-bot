# Telegram Bot Token (get from @BotFather)
TELEGRAM_API_TOKEN=your_bot_token_here

# Comma-separated list of enabled Telegram user IDs
# Example: ENABLED_USERS=123456789,987654321
# Use -999999 to disable authentication (NOT RECOMMENDED in production)
ENABLED_USERS=your_user_id_here

# Maximum task output size
MAX_TASK_OUTPUT=99999

# Port for the bot (if using webhooks with Heroku)
PORT=8443

# Proxy URL (optional)
# ALL_PROXY=http://proxy.example.com:8080
